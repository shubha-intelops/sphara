# Curl commands for REST Server resource UploadId
# Post
curl -X POST -H "Content-Type: application/json" \
-d '{"Id_type": "sample string","File": "sample string","Id_number": "sample string"}' \
http://localhost:8080/v1/uploadids

# Get
curl -X GET -H "Content-Type: application/json" \
http://localhost:8080/v1/uploadids

# Put
curl -X PUT -H "Content-Type: application/json" \
-d '{"Id": 123,"Id_number": "sample string","Id_type": "sample string","File": "sample string"}' \
http://localhost:8080/v1/uploadids/123

# Put
### wrong id passed in param
curl -X PUT -H "Content-Type: application/json" \
-d '{"Id": 123,"Id_number": "sample string","Id_type": "sample string","File": "sample string"}' \
http://localhost:8080/v1/uploadids/1234

# Get  (retrieve specific UploadId)
curl -X GET -H "Content-Type: application/json" \
http://localhost:8080/v1/uploadids/123

# Delete
curl -X DELETE -H "Content-Type: application/json" \
http://localhost:8080/v1/uploadids/123

    

