


    
# Curl commands for REST Server resource Signup
# Post
curl -X POST -H "Content-Type: application/json" \
-d '{"City": "sample string","Nickname": "sample string","Zip": 1,"Others": "sample string","Fullname": "sample string","R_address": "sample string","EmailId": "sample string","Country": "sample string","Dob": "sample string"}' \
http://localhost:8080/v1/signups

# Get
curl -X GET -H "Content-Type: application/json" \
http://localhost:8080/v1/signups

# Put
curl -X PUT -H "Content-Type: application/json" \
-d '{"Id": 123,"Country": "sample string","Dob": "sample string","Others": "sample string","Fullname": "sample string","R_address": "sample string","EmailId": "sample string","Nickname": "sample string","Zip": 1,"City": "sample string"}' \
http://localhost:8080/v1/signups/123

# Put
### wrong id passed in param
curl -X PUT -H "Content-Type: application/json" \
-d '{"Id": 123,"Country": "sample string","Dob": "sample string","Others": "sample string","Fullname": "sample string","R_address": "sample string","EmailId": "sample string","Nickname": "sample string","Zip": 1,"City": "sample string"}' \
http://localhost:8080/v1/signups/1234

# Get  (retrieve specific Signup)
curl -X GET -H "Content-Type: application/json" \
http://localhost:8080/v1/signups/123

# Delete
curl -X DELETE -H "Content-Type: application/json" \
http://localhost:8080/v1/signups/123

    
# Curl commands for REST Server resource MedicalCondition
# Post
curl -X POST -H "Content-Type: application/json" \
-d '{"File": "sample string","Medical_condition_details": "sample string"}' \
http://localhost:8080/v1/medicalconditions

# Get
curl -X GET -H "Content-Type: application/json" \
http://localhost:8080/v1/medicalconditions

# Put
curl -X PUT -H "Content-Type: application/json" \
-d '{"Id": 123,"File": "sample string","Medical_condition_details": "sample string"}' \
http://localhost:8080/v1/medicalconditions/123

# Put
### wrong id passed in param
curl -X PUT -H "Content-Type: application/json" \
-d '{"Id": 123,"File": "sample string","Medical_condition_details": "sample string"}' \
http://localhost:8080/v1/medicalconditions/1234

# Get  (retrieve specific MedicalCondition)
curl -X GET -H "Content-Type: application/json" \
http://localhost:8080/v1/medicalconditions/123

# Delete
curl -X DELETE -H "Content-Type: application/json" \
http://localhost:8080/v1/medicalconditions/123

    
